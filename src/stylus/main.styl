//main .styl

@import 'nib'
@import 'core'

// GENERIC
global-reset()
reset-html5()

canvas
	position absolute
	top 0
	left 0
	z-index 1
	background transparent

body
	background #F0F0F0
	min-height 100%
	height 100%
	width 100%

iframe
	z-index 1
	min-height 100%
	height 100%
	width calc(100% - 300px)
	position absolute
	top 0
	left 300px

@import 'menu.styl'
